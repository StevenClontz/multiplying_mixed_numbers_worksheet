\documentclass[11pt]{article}

\usepackage{fullpage}
\usepackage{graphicx}
\usepackage{amsmath}
\usepackage{amssymb}
\usepackage{amsthm}
\usepackage{fancyvrb}
\usepackage{multicol}
\usepackage{fancyhdr}

\parindent0in
\pagestyle{fancy}
\fancyhead{}
\cfoot{\tiny{
Generated by Clontz\LaTeX{} at <%= Time.now %>
}}

\newcounter{problemNumber}
\newenvironment{problem}{
  \stepcounter{problemNumber}
  \textbf{\theproblemNumber.}
}{
}

\newcommand{\printHeading}[1]{
\setcounter{problemNumber}{0}
\textbf{Mount Olive Intermediate School}
\hfill
\textbf{TEST}

\textbf{5th Grade Math}
\hfill
\textbf{Multiplying Fractions}

\textbf{Mrs. Clontz}
\hfill
\textbf{#1}

\smallskip\hrule\bigskip
}

\newcommand{\printProblems}{
<% problems = (0...10).to_a.map { MultiplicationProblem.new } %>
% Loop through problems
\begin{multicols}{2}
  <% problems.each do |problem| %>
    \begin{problem}
    $
    \displaystyle
    <%= problem.to_latex_s %>
    $
    \end{problem}

    \vspace{2em}
  <% end %>
\end{multicols}
}





\begin{document}

\printHeading{}

% hide solutions
\newcommand{\solution}[1]{}
\printProblems


\newpage
\printHeading{SOLUTIONS}



% show solutions
\renewcommand{\solution}[1]{\boxed{#1}}
\printProblems

\end{document}